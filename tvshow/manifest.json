{
  "manifest_version": 3,
  "name": "TV Shows",
  "description": "watch tv shows",
  "version": "1.0",
  "icons": {
    "16": "1.png",
    "48": "1.png",
    "128": "1.png"
  },
  "action": {
    "default_icon": {
      "26": "1.png",
      "48": "1.png",
      "128": "1.png"
    },
    "default_title": " TV Shows",
    "default_popup": "popup/popup.html"
  },
  "permissions": ["storage", "alarms", "notifications","tabs",  "contextMenus","search"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.netflix.com/*","https://www.geeksforgeeks.org/*","https://www.google.com/*" ,"https://www.youtube.com/*"],
      "css": ["content.css"],
      "js": ["content.js"]
    }
  ]
}
